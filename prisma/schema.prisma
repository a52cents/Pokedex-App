generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Team {
  id        Int      @id @default(autoincrement())
  name      String // Nom de l'Ã©quipe
  pokemons  String // Liste des pokemons de l'equipe 
  stats     String // stats globales de l'equipe
  types     String // differents types de l'equipe
  createdAt DateTime @default(now())
}
